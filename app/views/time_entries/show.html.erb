<h3>Time Entry</h3>
  <p>Matter: <%= link_to @time_entry.matter.name, matter_path(@time_entry.matter) %></p>
  <p>Client: <%= link_to @client_name, client_path(@time_entry.matter.client_id) %></p>
<hr>
  <p>Date: <%= @time_entry.date %></p>
  <p>Description: <%= @time_entry.description %></p>
<hr>
  <p>Duration: <%= @time_entry.duration %> hours</p>
  <p>Attorney: <%= @time_entry.lawyer.name %></p>
<hr>
  <p>Billable: <%= @time_entry.billable? ? "Yes" : "No" %></p>
  <p>Paid: <%= @time_entry.paid? ? "PAID" : "UNPAID" %></p>
<hr>
<%= button_to 'Edit time entry', edit_time_entry_path(@time_entry), method: "get" %>
