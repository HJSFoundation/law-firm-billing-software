<h1>New Time Entry</h1>

<%= form_for(@time_entry) do |f| %>

  <%= f.label :matter %><br>
  <%= f.text_field :matter%><br>
  <%= f.label :date %><br>
  <%= f.text_field :date %><br>
  <%= f.label :duration %><br>
  <%= f.text_field :duration %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :lawyer %><br>
  <%= f.text_field :lawyer %><br>
  <%= f.label :billable %>
  <%= f.check_box :billable %>
  <%= f.label :paid %>
  <%= f.check_box :paid %><br><br>
  <%= f.submit %>

<% end %>
