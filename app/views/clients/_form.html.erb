<%= form_for(client) do |f| %>

  <%= render partial: "layouts/errors", locals: {instance: client} %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :address %><br>
  <%= f.text_field :address %><br>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br><br>
  <%= f.submit %>

<% end %><br>
